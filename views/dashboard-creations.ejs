    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const nftMetadata = JSON.parse(sessionStorage.getItem('nftMetadata'));
            const nftGallery = document.getElementById('nft-gallery');

            if (nftMetadata) {
                nftMetadata.forEach(nft => {
                    const nftElement = document.createElement('a');
                    nftElement.classList.add('nft-item');
                    nftElement.href = nft.metadata.image;
                    nftElement.setAttribute('data-fancybox', 'gallery');
                    nftElement.setAttribute('data-caption', nft.metadata.name);
                    nftElement.innerHTML = `<img src="${nft.metadata.image}" alt="${nft.metadata.name}"> <p class="nft-name">Name: ${nft.metadata.name}</p> <p class="nft-name">Symbol: ${nft.metadata.symbol}</p> <p class="nft-name">description: ${nft.metadata.description}</p>`;
                    nftGallery.appendChild(nftElement);
                });
            }
        });
    </script>

